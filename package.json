{
  "name": "eslint-config",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/shinlms404/eslint-config#readme",
  "bugs": "https://github.com/shinlms404/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinlms404/eslint-config.git"
  },
  "license": "MIT",
  "author": "Charlie Chan <shinlms404@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "prepack": "nr build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "stub": "tsup --format esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --format esm,cjs --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsonc": "^2.11.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "bumpp": "^9.2.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.35.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "packageManager": "pnpm@8.14.1",
  "publishConfig": {
    "access": "public"
  }
}
